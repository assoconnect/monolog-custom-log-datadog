        "koriym/http-constants": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "roave/no-floaters": "^1.5",
        "ext-json": "*"
    }
}
