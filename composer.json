    "description": "Monolog handler for Datadog",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AssoConnect\\MonologDatadog\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.3",
